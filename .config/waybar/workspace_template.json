{
  "hyprland/workspaces": {
    "all-outputs": false,
    "active-only": false,
    "persistent-workspaces": {
      "eDP-1": [6, 7, 8, 9, 10],
      "DP-3": [1, 2, 3, 4, 5]
    },
    "window-rewrite": {
      "title<.*youtube.*>": "󰗃",
      "class<firefox>": "",
      "class<firefox> title<.*github.*>": "",
      "class<firefox> title<.*gitlab.*>": "",
      "title<.*nvim.*>": "",
      "class<Alacritty>": "",
      "class<Slack>": "",
      "title<Microsoft Teams.*>": "\ue533",
      "class<Thunderbird>": "\uf0e0"
    },
    "format": "{name}: {windows} ",
    "format-window-separator": " ",
    "window-rewrite-default": ""
  },
  "hyprland/window": {
    "rewrite": {
      "(.*) — Mozilla Firefox": "🌎 $1",
      "(.*) - Slack": "  $1"
    },
    "separate-outputs": true
  }
}
